generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  user_name String
  password  String
}

model File {
  id        Int      @id @default(autoincrement())
  url       String
  createdAt DateTime @default(now())
}
