{
	"name": "pastebin",
	"version": "1.0.0",
	"description": "Pastebin API",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun run --watch ./src/index.ts",
		"start": "bun ./dist/bundle.js",
		"start:migrate": "bunx prisma migrate deploy && bun run start",
		"build": "bun build ./src/index.ts --outfile ./dist/bundle.js --minify --target bun",
		"build:dev": "bun build ./src/index.ts --outfile ./dist/bundle.js --minify --sourcemap=inline --target bun",
		"prisma:generate": "bunx prisma generate",
		"prisma:migrate": "bunx prisma migrate dev",
		"prisma:studio": "bunx prisma studio",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier --ignore-path .gitignore --check src",
		"prettier:fix": "prettier --write src",
		"clean": "rm -rf dist"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/bun": "^1.1.14",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/ioredis": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node-cron": "^3.0.11",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"eslint": "^9.17.0",
		"prettier": "^3.4.2",
		"prisma": "^6.1.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.2"
	},
	"dependencies": {
		"@prisma/client": "^6.1.0",
		"@aws-sdk/client-s3": "^3.717.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"express": "^4.21.2",
		"express-validator": "^7.2.0",
		"ioredis": "^5.4.2",
		"jsonwebtoken": "^9.0.2",
		"node-cron": "^3.0.3"
	},
	"license": "MIT"
}
