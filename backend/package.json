{
	"name": "pastebin",
	"version": "1.0.0",
	"description": "Pastebin API",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun run --watch ./src/index.ts",
		"start": "bun ./dist/bundle.js",
		"prisma:migrate": "bunx prisma migrate deploy",
		"build": "bun build ./src/index.ts --outfile ./dist/bundle.js --minify --target bun",
		"build:dev": "bun build ./src/index.ts --outfile ./dist/bundle.js --minify --sourcemap=inline --target bun"
	},
	"devDependencies": {
		"@types/bun": "^1.2.5",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/ioredis": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.9",
		"prisma": "^6.5.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.758.0",
		"@prisma/client": "^6.5.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"express": "^4.21.2",
		"express-validator": "^7.2.1",
		"ioredis": "^5.6.0",
		"jsonwebtoken": "^9.0.2"
	},
	"license": "MIT"
}
